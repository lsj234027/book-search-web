{
  "name": "book-search-web",
  "version": "0.1.0",
  "private": true,
  "scripts": { 
    "start": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "qs": "^6.7.0",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.30",
    "vue-router": "^3.0.6",
    "vuex": "^3.0.1",
    "uuid": "latest",
    "lodash": "latest"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "lines-between-class-members": "off",
      "comma-dangle": "off",
      "no-param-reassign": 0,
      "no-underscore-dangle": "off",
      "import/extensions": [
        "error",
        {
          "js": "never",
          "vue": "never"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
